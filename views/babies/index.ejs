<% include ../partials/header %>

<h1>View and edit babies</h1>
<table class="table">
<%babies.forEach(function(baby){%>
<tr>
<td>
	<%= baby.name%>
	</td>
	<td>
	<img style="width:50px" src="<%= baby.image%>" alt="">
	</td>
	<td>
	<form action="/babies/<%= baby._id%>?_method=DELETE" method="POST">
      <input type="submit" value="DELETE">
    </form>
	</td>
	<%})  %>

	</tr>
	</table>

<% include ../partials/footer %>